        <Button x:Name="btnPrevious"
                HorizontalAlignment="Left"
                Margin="289,59,0,0"
                VerticalAlignment="Top"
                Width="89"
                Height="31"
                Background="White"
                FontSize="20"
                BorderBrush="{x:Null}"
                Padding="0"
                Command="{Binding btnClickCmd}"
                CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}}">
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="MouseEnter">
                    <i:InvokeCommandAction Command="{Binding Path=btnEnterCmd}"
                                           CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Button}}" />
                </i:EventTrigger>
                <i:EventTrigger EventName="MouseLeave">
                    <i:InvokeCommandAction Command="{Binding Path=btnLeaveCmd}"
                                           CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Button}}" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
            <Button.Content>
                <DockPanel>
                    <Grid DockPanel.Dock="Left">
                        <Image x:Name="prevImg"
                               Source="../images/arrow.png"
                               Width="20"
                               RenderTransformOrigin="0.5,0.5">
                            <Image.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform />
                                    <SkewTransform />
                                    <RotateTransform Angle="180" />
                                    <TranslateTransform />
                                </TransformGroup>
                            </Image.RenderTransform>
                        </Image>
                        <Button x:Name="prevbtnIcon"
                                Style="{StaticResource mouseOverStyle}"
                                Background="{Binding MouseOverPrevBtn, Converter={StaticResource ResourceKey=isMouseOverCnv}}"
                                BorderThickness="0"
                                Width="{Binding ActualWidth, Source={x:Reference Name=prevImg}}"
                                Height="{Binding ActualHeight, Source={x:Reference Name=prevImg}}" />
                    </Grid>
                    <Label Content="PREVIOUS"
                           Foreground="#FF8671CE"
                           DockPanel.Dock="Right"
                           FontSize="14"
                           Height="20"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           Padding="0"
                           Margin="3,0,0,0" />
                </DockPanel>
            </Button.Content>
        </Button>
