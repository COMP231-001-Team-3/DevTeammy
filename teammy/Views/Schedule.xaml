                           FontSize="14"
                           Height="19"
                           Padding="0"
                           Width="33"
                           HorizontalAlignment="Center"
                           VerticalContentAlignment="Center"
                           Margin="0,0,3,0" />
                </DockPanel>
            </Button.Content>
